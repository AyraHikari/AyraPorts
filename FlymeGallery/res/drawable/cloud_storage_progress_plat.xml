<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="4.1999817dip" />
            <solid android:color="@color/black_5" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="4.1999817dip" />
                <gradient android:startColor="#ff3b3b40" android:endColor="#ff84847b" android:angle="0.0" />
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="4.1999817dip" />
                <solid android:color="@color/cloud_full_color" />
            </shape>
        </scale>
    </item>
</layer-list>