<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="@dimen/mz_seekbar_height" android:top="@dimen/mz_seekbar_background_padding_top">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/mz_seekbar_radius" />
            <solid android:color="@color/mz_seekbar_background_color" />
        </shape>
    </item>
    <item android:gravity="start" android:height="@dimen/mz_seekbar_height" android:width="11.0dip" android:top="@dimen/mz_seekbar_background_padding_top">
        <shape android:shape="rectangle">
            <corners android:topRightRadius="@dimen/mz_seekbar_radius" android:bottomRightRadius="@dimen/mz_seekbar_radius" />
            <solid android:color="@color/mz_seekbar_thumb_color" />
        </shape>
    </item>
</layer-list>